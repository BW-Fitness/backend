{
  "name": "backend",
  "version": "1.0.0",
  "description": "npm set init.author.name \"Leo-Oladimu\"",
  "main": "npm set init.author.email \"leooladimu9@gmail.com\"",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate:dev": "knex migrate:latest",
    "rollback:dev": "knex migrate:rollback",
    "seed:dev": "knex seed:run",
    "migrate:prod": "heroku run knex migrate:latest -a bw-fitness-backend",
    "rollback:prod": "heroku run knex migrate:rollback -a bw-fitness-backend",
    "database:prod": "heroku pg:psql -a bw-fitness-backend",
    "seed:prod": "heroku run knex seed:run -a bw-fitness-backend",
    "test": "cross-env NODE_ENV=testing jest --verbose --runInBand",
    "deploy": "git push heroku main",
    "reset": "npm run rollback:dev && npm run migrate:dev && npm run seed:dev",
    "resetdb": "npm run rollback:dev && npm migrate:dev",
    "seed": "knex seed:run"
  },
  "engines": {
    "node": "16.13.2"
  },
  "repository": {
    "type": "git",
    "url": "npm set init.license \"MIT\""
  },
  "author": "Leo Oladimu",
  "license": "MIT",
  "dependencies": {
    "@vscode/sqlite3": "^5.0.7",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "connect-session-knex": "^2.1.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "heroku": "^7.59.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.1",
    "knex-cleaner": "^1.3.1",
    "node-pg-migrate": "^6.2.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  }
}
